<changeSet id="05-create-logs-table" author="your_name">
    <createTable tableName="logs" schemaName="service_schema">
        <column name="id" type="BIGSERIAL">
            <constraints primaryKey="true"/>
        </column>
        <column name="timestamp" type="TIMESTAMP">
            <constraints nullable="false"/>
        </column>
        <column name="log_message" type="VARCHAR(255)">
            <constraints nullable="false"/>
        </column>
        <column name="level" type="VARCHAR(50)">
            <constraints nullable="false"/>
        </column>
    </createTable>
</changeSet>

<changeSet id="02-insert-log-data" author="your_name">
<insert tableName="logs" schemaName="service_schema">
    <column name="timestamp" value="CURRENT_TIMESTAMP"/>
    <column name="log_message" value="System started"/>
    <column name="level" value="INFO"/>
</insert>
</changeSet>